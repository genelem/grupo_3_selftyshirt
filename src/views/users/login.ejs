  <%- include('../partials/head') %>
  <head>
    <link rel="stylesheet" href="css/index.css" />
    <link rel="stylesheet" href="css/login.css" />
    <title>Iniciar Sesion - Selfty Shirt</title>
  </head>

    <%- include('../partials/header') %>

  <body>
    <main class="register">

      <div class="datos-registro">
        <div class="logo-ctn">
        <img src="images/logo.png" alt="" class="pic" />
      </div>
        <h2>Bienvenido!</h2>
        <h3>Ingresar con correo electronico:</h3>
      </div>
      <form action="/" method="POST">
        <input type="email" name="email" placeholder="Correo Electronico" />
        <br />
        <input type="password" name="pass" placeholder="Contraseña" />
        <br />
        <br />
        <div class="recordar-check"><label><input type="checkbox" id="remember" value="true"> Recordar mi cuenta</label></div>
      </div>
      <div class="google_button">
        <img src="https://upload.wikimedia.org/wikipedia/commons/5/53/Google_%22G%22_Logo.svg" alt="">
        <p>Continuar con Google</p>
      </div>
      <br>
        <input type="submit" value="Ingresar" />
        <br />
        ¿No tienes una cuenta? <a href="/register" class="a_top_hypers"><span style="color: blue;">Regístrate</a>
        <br />
      </form>
    </main>
    <%- include('../partials/footer') %>
  </body>
</html>
